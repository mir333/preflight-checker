{
  "id": "spousteni_motoru_cold_en",
  "title": "Engine Start (Cold Engine)",
  "steps": [
    { "text": "Seat belts - fastened and tightened" },
    { "text": "Controls - free" },
    { "text": "Cabin cover - closed and secured" },
    { "text": "Parking brake - engaged" },
    { "text": "Throttle - idle" },
    { "text": "Choke - fully open" },
    { "text": "Master switch - on" },
    { "text": "Fuel pump P2 - on" },
    { "text": "Propeller - fine" },
    { "text": "Lights, beacon - on" },
    { "text": "Area around gyro and propeller - clear" },
    { "text": "Magnetos - on" },
    { "text": "Starter - engaged" },
    { "text": "After start, oil pressure - min 1.5 bar" },
    { "text": "Fuel pump P2 - off" },
    { "text": "Instruments/Radio - on" },
    { "text": "Choke - gradually close" }
  ]
}
