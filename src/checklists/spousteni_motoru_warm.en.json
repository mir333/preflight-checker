{
  "id": "spousteni_motoru_warm_en",
  "title": "Engine Start (Warm Engine)",
  "description": "",
  "steps": [
    { "id": 1, "text": "Seat belts - fastened and tightened" },
    { "id": 2, "text": "Controls - free" },
    { "id": 3, "text": "Cabin cover - closed and secured" },
    { "id": 4, "text": "Parking brake - engaged" },
    { "id": 5, "text": "Throttle - idle or slightly open" },
    { "id": 6, "text": "Choke - closed" },
    { "id": 7, "text": "Master switch - on" },
    { "id": 8, "text": "Fuel pump P2 - on" },
    { "id": 9, "text": "Propeller - fine" },
    { "id": 10, "text": "Lights, beacon - on" },
    { "id": 11, "text": "Area around gyro and propeller - clear" },
    { "id": 12, "text": "Magnetos - on" },
    { "id": 13, "text": "Starter - engaged" },
    { "id": 14, "text": "After start, oil pressure - min 1.5 bar" },
    { "id": 15, "text": "Fuel pump P2 - off" },
    { "id": 16, "text": "Instruments/Radio - on" },
    { "id": 17, "text": "Choke - gradually close" }
  ]
}
